{
  "name": "cyaris.github.io",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.{css,cjs,html,js,json,svelte,svg}'",
    "format:check": "prettier --ignore-path ../.gitignore --check '**/*.{css,cjs,html,js,json,svelte,svg}'",
    "lint": "eslint '**/*.{cjs,html,js,svelte}'",
    "lint:fix": "eslint --fix '**/*.{cjs,html,js,svelte}'"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-css": "^0.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-svelte3": "^3.4.0",
    "prettier": "^2.8.6",
    "prettier-plugin-svelte": "^2.10.0",
    "@shopify/prettier-plugin-liquid": "^1.2.1"
  }
}
